{
    "framework": "vite",
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "functions": {
        "api/upload.js": {
            "maxDuration": 60,
            "memory": 1024
        }
    },
    "rewrites": [
        {
            "source": "/api/upload",
            "destination": "/api/upload"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}